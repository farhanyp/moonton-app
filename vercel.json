{
    "version": 2,
    "framework": null,
    "routes": [
        { "src": "/build/(.*)", "dest": "/public/build/" },
        { "src": "/(.*)", "dest": "/api/index.php" },
        {  "src": "/(.*)", "dest": "public/index.php"}
    ],
    "builds": [
        {
          "src": "public/index.php",
          "use": "@vercel/php",
          "config": { "includeFiles": ["**/*.php"] }
        },
        {
          "src": "resources/js/**/*.js",
          "use": "@vercel/node"
        },
        {
          "src": "resources/css/**/*.css",
          "use": "@vercel/node"
        }
      ],
    "public": false,
    "buildCommand": "vite build",
    "outputDirectory": "public",
    "env": {
        "APP_ENV": "production",
        "APP_DEBUG": "true",
        "APP_URL": "https://moonton.vercel.app",
        "APP_KEY": "base64:zhEVnypUBjMdWQQS+qMeZH3kNQDNqUsOsOgNiyv5fes=",
        "APP_CONFIG_CACHE": "/tmp/config.php",
        "APP_EVENTS_CACHE": "/tmp/events.php",
        "APP_PACKAGES_CACHE": "/tmp/packages.php",
        "APP_ROUTES_CACHE": "/tmp/routes.php",
        "APP_SERVICES_CACHE": "/tmp/services.php",
        "VIEW_COMPILED_PATH": "/tmp",
        "CACHE_DRIVER": "file",
        "LOG_CHANNEL": "stack",
        "SESSION_DRIVER": "file",
        "DB_CONNECTION": "mysql",
        "DB_HOST": "8ur.h.filess.io",
        "DB_PORT": "3306",
        "DB_DATABASE": "moonton_youravoid",
        "DB_USERNAME": "moonton_youravoid"
    }
}